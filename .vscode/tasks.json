{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build",
            "type": "shell",
            "command": "/u/prod/bin/mtimake",
            "args": [
                "--remote",
                "-enable-redis"
            ],
            "options": {
                "cwd": "${workspaceFolder}/build/linux_x86_64"
            },
            "problemMatcher": [
                {
                    "base": "$gcc",
                    "background": {},
                    "fileLocation": [
                        "autoDetect",
                        "${workspaceFolder}/build/linux_x86_64"
                    ]
                },
                {
                    "pattern": {
                        "regexp": "^(.*):(\\d+):(\\d+):\\s+(required from here)$",
                        "file": 1,
                        "line": 2,
                        "column": 3,
                        "message": 4
                    },
                    "fileLocation": [
                        "autoDetect",
                        "${workspaceFolder}/build/linux_x86_64"
                    ],
                    "owner": "cpptools"
                }
            ],
            "presentation": {
                "reveal": "always",
                "revealProblems": "onProblem",
                "clear": true
            },
            "group": "build"
        },
